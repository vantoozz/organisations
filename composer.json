{
  "name": "vantoozz/organisations-test",
  "description": "Organisations test",
  "license": "MIT",
  "require": {
    "php": ">=5.6.0",
    "laravel/lumen-framework": "~5",
    "doctrine/dbal": "~2",
    "illuminate/redis": "~5",
    "predis/predis": "~1",
    "vlucas/phpdotenv": "~2",
    "filp/whoops": "~2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1",
    "phploc/phploc": "~3",
    "phpunit/phpunit": "~5",
    "squizlabs/php_codesniffer": "~2",
    "sebastian/phpcpd": "~2",
    "phpmd/phpmd": "~2",
    "block8/php-docblock-checker": "~1",
    "jakub-onderka/php-parallel-lint": "~0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "classmap": [
      "database/",
      "tests/TestCase.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/unit",
      "App\\Tests\\Integration\\": "tests/integration"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "post-install-cmd": [
      "bash bin/setup.sh"
    ],
    "post-update-cmd": [
      "bash bin/setup.sh"
    ]
  }
}
